{
  "name": "react-redux-typescript-boilerplate",
  "version": "1.0.1",
  "private": true,
  "description": "A frontend boilerplate with React, Redux and Typescript",
  "homepage": "https://rokoroku.github.io/react-redux-typescript-boilerplate",
  "keywords": [
    "react",
    "redux",
    "typescript",
    "boilerplate",
    "training project"
  ],
  "main": "index.js",
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "test": "jest --watch",
    "start": "webpack-dev-server --mode development --hot --progress --color --port 3000",
    "build": "webpack -p --progress --colors",
    "prettier": "prettier --write \"src/**/*.{ts,tsx,css}\""
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/classnames": "^2.2.7",
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/history": "^4.7.2",
    "@types/jest": "^24.0.15",
    "@types/node": "^10.12.18",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "@types/react-redux": "^7.0.0",
    "@types/react-router": "^4.4.3",
    "@types/redux-actions": "2.3.1",
    "@types/sinon": "^7.0.13",
    "@types/webpack": "^4.4.23",
    "@types/webpack-env": "1.13.6",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^2.0.1",
    "css-loader": "^1.0.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.3.5",
    "file-loader": "^3.0.1",
    "gh-pages": "^2.0.1",
    "html-loader": "^1.0.0-alpha.0",
    "html-webpack-plugin": "^4.0.0-alpha",
    "jest": "^24.8.0",
    "jest-environment-enzyme": "^7.0.2",
    "jest-enzyme": "^7.0.2",
    "mini-css-extract-plugin": "^0.5.0",
    "postcss": "^7.0.13",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.5.0",
    "postcss-reporter": "^6.0.1",
    "postcss-url": "^8.0.0",
    "prettier": "^1.15.3",
    "raf": "^3.4.1",
    "react-hot-loader": "^4.6.3",
    "redux-devtools-extension": "^2.13.7",
    "sinon": "^7.3.2",
    "style-loader": "^0.23.1",
    "ts-jest": "^24.0.2",
    "ts-loader": "^5.3.3",
    "typescript": "^3.5.3",
    "typings-for-css-modules-loader": "^1.7.0",
    "url-loader": "^1.1.2",
    "webpack": "^4.36.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  },
  "dependencies": {
    "@types/google-map-react": "^0.23.12",
    "@types/lodash": "^4.14.136",
    "@types/react-datepicker": "^2.3.0",
    "@types/react-router-dom": "^4.3.4",
    "@types/styled-components": "^4.1.18",
    "awesome-typescript-loader": "^5.2.1",
    "classnames": "^2.2.6",
    "google-map-react": "^1.1.4",
    "lodash": "^4.17.19",
    "moment": "^2.24.0",
    "query-string": "^6.8.1",
    "react": "^16.8.6",
    "react-datepicker": "^2.8.0",
    "react-dates": "^20.2.5",
    "react-day-picker": "^7.3.0",
    "react-dom": "^16.8.6",
    "react-redux": "^6.0.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.4",
    "redux-mock-store": "^1.5.3",
    "redux-thunk": "^2.3.0",
    "redux-thunk-actions": "^1.3.2",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.87.3",
    "source-map-loader": "^0.2.4",
    "styled-components": "^4.3.2"
  },
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.(ts|tsx)",
      "!**/node_modules/**"
    ],
    "coverageDirectory": "<rootDir>/coverage",
    "setupFiles": [
      "<rootDir>/test-shim.js",
      "<rootDir>/test-setup.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/test-preprocessor.js"
    },
    "testMatch": [
      "**/__tests__/*.(ts|tsx|js)"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/assetsTransformer.js",
      "\\.(css|less)$": "<rootDir>/assetsTransformer.js"
    }
  }
}
